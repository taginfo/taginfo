<div class="pre">
    <h1 class="section"><%= @title %></h1>
    <p><b><%= t.pages.search.results.you_were_searching_for %>:</b> <%= escape_html(@query) %></p>
</div>
<div id="tabs">
    <ul>
        <li><a href="#keys"><%= t.osm.keys %></a></li>
        <li><a href="#values"><%= t.osm.values %></a></li>
    </ul>
    <div id="keys">
        <h2><%= t.osm.keys %></h2>
        <table id="grid-keys">
        </table>
    </div>
    <div id="values">
        <h2><%= t.osm.values %></h2>
        <table id="grid-values">
        </table>
    </div>
</div>
<% javascript do
    JS.raw(%Q{
function page_init() {
    init_tabs('search', [#{ @query.to_json }]);
}
    })
end
%>
